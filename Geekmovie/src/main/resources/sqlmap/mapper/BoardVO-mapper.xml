<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace : package + classname -->
<mapper namespace="com/geekmovie/board/vo/BoardVo.java">
 <resultMap type="BoardVO" id="BoardVO">

		 <result property="seq" column="seq" /> <!-- 게시글 고유번호 -->
		 <result property="title" column="title" />
		 <result property="writer" column="writer" />
		 <result property="contents" column="contents" /> <!-- 게시글 내용 -->
		 <result property="reply" column="reply" /> <!-- 게시글 댓글 -->
		 <result property="redate" column="regdate" />
		 <result property="cnt" column="cnt" /> <!-- 게시글 조회수 -->
		 <result property="likes" column="likes" /> <!-- 게시글 추천수 -->

	 </resultMap>

</mapper>